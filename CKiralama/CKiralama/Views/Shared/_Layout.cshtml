<!DOCTYPE html>
<html lang="en">
<head>
    <title>@(Html.Raw(ViewBag.Title))</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- MAIN CSS -->
    <link rel="stylesheet" href="@Url.Content("~/Content/Site/css/style.css")">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    


</head>
<body>

    <nav class="navbar navbar-inverse">
        <a class="navbar-brand" href="@Url.Action("","")"><img src="data:image/png;base64," width="250" height="36" "alt=" Web Sitesi Logosu" </a>
        <div class="container-fluid">
            <div class="navbar-header">

            </div>
            <ul class="nav navbar-nav">
                <li>
                    <a href="@Url.Action("","")">Anasayfa</a>

                </li>


                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="@Url.Action("Index","Cars")">Araçlar <span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li><a href="@Url.Action("EkonomikCars","Cars")">Ekonomik Paket Araçlar</a></li>
                        <li><a href="@Url.Action("OrtaCars","Cars")">Orta Paket Araçlar</a></li>
                        <li><a href="@Url.Action("LuxsCars","Cars")">Lüks Paket Araçlar</a></li>
                    </ul>
                </li>

                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="@Url.Action("Index","Station")">Ofislerimiz <span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li><a href="@Url.Action("Durak1","Station")">istanbul</a></li>
                        <li><a href="@Url.Action("Durak2","Station")">Ankara</a></li>
                        <li><a href="@Url.Action("Durak3","Station")">Antalya</a></li>


                    </ul>

                
                <li>@Html.ActionLink("Hakkımızda", "About", "Home")</li>
                <li>@Html.ActionLink("İletişim", "Contact", "Home")</li>



            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li><a href="@Url.Action("Ekle","Kayıt")"><span class="glyphicon glyphicon-user"></span> Üye Ol</a></li>
                <li><a href="@Url.Action("Giris","Giris")"><span class="glyphicon glyphicon-log-in"></span> Giriş Yap</a></li>
                

            </ul>

        </div>
    </nav>

    <div>
        @RenderBody()
    </div>


    <br />
    <br />
    <br />
    <br />






    <footer class="site-footer">

        <div class="container">
            <div class="row">
                <div class="col-lg-3">
                    <h2 class="footer-heading mb-4">Proje</h2>
                    <p>CD Rent Car  Tarafından 2022 Lisans ile Geliştirilmiştir.. </p>

                </div>
            </div>
        </div>
    </footer>




    <script src="@Url.Content("~/Content/Site/js/jquery-3.3.1.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/popper.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/owl.carousel.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/jquery.sticky.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/jquery-migrate-3.0.1.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/jquery-ui.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/jquery.easing.1.3.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/jquery.countdown.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/jquery.magnific-popup.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/jquery.stellar.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/jquery.waypoints.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/mediaelement-and-player.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/slick.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/typed.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/jquery.animateNumber.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/jquery.fancybox.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/js/jquery.easing.1.3.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/bootstrap-datepicker.min.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/aos.js")"></script>
    <script src="@Url.Content("~/Content/Site/js/main.js")"></script>




















</body>

</html>







